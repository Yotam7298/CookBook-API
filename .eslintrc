{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "linebreak-style": "off",
    "keyword-spacing": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "max-classes-per-file": ["error", 5]
  }
}
